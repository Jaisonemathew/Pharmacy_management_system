<% include inc/view_header %>
    <% include inc/view_topmenu %>
        <% include inc/view_sidebar %>

            <div id="page-wrapper">

                <div class="container-fluid">

                    <!-- Page Heading -->
                    <div class="row">
                        <div class="col-lg-12">
                            <h1 class="page-header text-center">
                                Edit Supplier
                            </h1>
                        </div>
                    </div>
                    <!-- /.row -->

                    <div class="row">
                        <div class="col-lg-5 col-lg-offset-3">
                            <form action="" method="post">
                                <div class="form-group">
                                    <label for="med_name">Supplier Name</label>
                                    <input type="text" class="form-control" id="supplier_name" name="supplier_name" value="<%= supplierEdit.Supplier_Name %>">
                                </div>
                                <div class="form-group">
                                    <label for="med_category">Contact</label>
                                    <input type="text" class="form-control" name="contact" id="contact" value="<%= supplierEdit.Contact %>">
                                </div>
                                <div class="form-group">
                                    <label for="gen_name">Email</label>
                                    <input type="email" class="form-control" name="email" id="email" value="<%= supplierEdit.Email %>">
                                </div>
                                <br/>
                                <input type="submit" class="btn btn-primary" name="submit" value="Update">
                            </form>

                            <% include inc/alert_msg.ejs %>

                            
                        </div>
                    </div>

                    <br />
                    <br />
                    <div class="container" style="float: left; font-size:17px;">
                        <a href="/admin/supplier">Back to List </a>
                    </div>
                    <!-- /.row -->
                    <br>
                    <hr>

                </div>
                <!-- /.container-fluid -->

            </div>
            <!-- /#page-wrapper -->

            <% include inc/view_footer.ejs %>